{
  "name": "hive-data-graph",
  "version": "0.5.2",
  "description": "data and such",
  "author": "Elbasan Kadrija",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=22.15"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "TZ=UTC nest start",
    "start:prod": "TZ=UTC node dist/main",
    "dev": "TZ=UTC nest start --watch",
    "debug": "TZ=UTC nest start --debug --watch",
    "docker:start": "docker-compose up -d && exit 0",
    "check": "biome check --write .",
    "lint": "biome lint --write .",
    "format": "biome format --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky",
    "migration": "sh .scripts/migration-handler.sh"
  },
  "dependencies": {
    "@apollo/server": "4.12.1",
    "@as-integrations/fastify": "2.1.1",
    "@azure/cosmos": "4.3.0",
    "@azure/logger": "1.2.0",
    "@fastify/accepts": "5.0.2",
    "@fastify/cors": "11.0.1",
    "@fastify/helmet": "13.0.1",
    "@liaoliaots/nestjs-redis": "10.0.0",
    "@nestjs/apollo": "13.1.0",
    "@nestjs/axios": "4.0.0",
    "@nestjs/cache-manager": "3.0.1",
    "@nestjs/common": "11.1.0",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.1.0",
    "@nestjs/devtools-integration": "0.2.0",
    "@nestjs/graphql": "13.1.0",
    "@nestjs/jwt": "11.0.0",
    "@nestjs/passport": "11.0.5",
    "@nestjs/platform-fastify": "11.1.0",
    "@nestjs/schedule": "6.0.0",
    "@nestjs/terminus": "11.0.0",
    "@nestjs/typeorm": "11.0.0",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/auto-instrumentations-node": "0.58.1",
    "@opentelemetry/exporter-jaeger": "2.0.0",
    "@opentelemetry/exporter-prometheus": "0.200.0",
    "@opentelemetry/exporter-trace-otlp-http": "0.200.0",
    "@opentelemetry/instrumentation-winston": "0.45.0",
    "@opentelemetry/sdk-node": "0.200.0",
    "@opentelemetry/semantic-conventions": "1.32.0",
    "axios": "1.9.0",
    "axios-cache-adapter": "2.7.3",
    "cache-manager": "6.4.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.2",
    "countries-list": "3.1.1",
    "crypto-js": "4.2.0",
    "dataloader": "2.2.3",
    "date-fns": "4.1.0",
    "date-fns-tz": "3.2.0",
    "graphql": "16.11.0",
    "graphql-query-complexity": "1.1.0",
    "graphql-type-json": "0.3.2",
    "ioredis": "5.6.1",
    "json-stream-stringify": "^3.1.6",
    "mssql": "11.0.1",
    "nest-winston": "1.10.2",
    "nestjs-otel": "6.2.0",
    "passport": "0.7.0",
    "passport-custom": "1.1.1",
    "passport-jwt": "4.0.1",
    "pg": "8.15.6",
    "reflect-metadata": "0.2.2",
    "rimraf": "6.0.1",
    "rxjs": "7.8.2",
    "semver": "7.7.1",
    "systeminformation": "5.25.11",
    "typeorm": "0.3.23",
    "typeorm-naming-strategies": "4.1.0",
    "winston": "3.17.0"
  },
  "devDependencies": {
    "@babel/traverse": "7.27.1",
    "@biomejs/biome": "1.9.4",
    "@faker-js/faker": "9.7.0",
    "@golevelup/ts-jest": "0.6.2",
    "@nestjs/cli": "11.0.7",
    "@nestjs/schematics": "11.0.5",
    "@nestjs/testing": "11.1.0",
    "@types/cache-manager": "5.0.0",
    "@types/crypto-js": "4.2.2",
    "@types/express": "5.0.1",
    "@types/jest": "29.5.14",
    "@types/node": "22.15.14",
    "@types/passport-jwt": "4.0.1",
    "@types/supertest": "6.0.3",
    "@vercel/cosmosdb-server": "0.15.1",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "lint-staged": "15.5.2",
    "source-map-support": "0.5.21",
    "supertest": "7.1.0",
    "ts-jest": "29.3.2",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.8.3"
  },
  "resolutions": {
    "formidable": "3.5.3",
    "@babel/traverse": "7.27.1",
    "@babel/helpers": "7.27.1"
  },
  "jest": {
    "globalSetup": "../test/globalSetup.ts",
    "moduleFileExtensions": ["js", "json", "ts"],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": ["**/*.(service|guard).(t|j)s", "**/utils/index.ts"],
    "coverageReporters": ["cobertura", "clover"],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "./coverage",
          "outputName": "junit.xml"
        }
      ]
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@src/(.*)$": ["<rootDir>/../src/$1"],
      "^@auth/(.*)$": ["<rootDir>/../src/auth/$1"],
      "^@endpoint/(.*)$": ["<rootDir>/../src/endpoints/$1"],
      "^@shared/(.*)$": ["<rootDir>/../src/shared/$1"],
      "^@utils/(.*)$": ["<rootDir>/../src/utils/$1"]
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts}": ["yarn format", "yarn biome check --staged", "yarn test --passWithNoTests"],
    "package.json": ["yarn format"]
  }
}
